<!-- README!
	For Notepad++, double-click on the very bottom of the window with a text file open (it should say "Normal text file")
	Then select "Define your language"
	And from there select "Import" and browse to this file on your PC
 -->
<NotepadPlus>
    <UserLang name="Phrancis Notes Markup" ext="" udlVersion="2.1">
        <Settings>
            <Global caseIgnored="no" allowFoldOfComments="yes" foldCompact="no" forcePureLC="0" decimalSeparator="0" />
            <Prefix Keywords1="no" Keywords2="no" Keywords3="no" Keywords4="no" Keywords5="no" Keywords6="no" Keywords7="yes" Keywords8="no" />
        </Settings>
        <KeywordLists>
            <Keywords name="Comments">00// 01 02 03{ 04}</Keywords>
            <Keywords name="Numbers, prefix1"></Keywords>
            <Keywords name="Numbers, prefix2"></Keywords>
            <Keywords name="Numbers, extras1"></Keywords>
            <Keywords name="Numbers, extras2"></Keywords>
            <Keywords name="Numbers, suffix1"></Keywords>
            <Keywords name="Numbers, suffix2"></Keywords>
            <Keywords name="Numbers, range"></Keywords>
            <Keywords name="Operators1"></Keywords>
            <Keywords name="Operators2"></Keywords>
            <Keywords name="Folders in code1, open"></Keywords>
            <Keywords name="Folders in code1, middle"></Keywords>
            <Keywords name="Folders in code1, close"></Keywords>
            <Keywords name="Folders in code2, open"></Keywords>
            <Keywords name="Folders in code2, middle"></Keywords>
            <Keywords name="Folders in code2, close"></Keywords>
            <Keywords name="Folders in comment, open"></Keywords>
            <Keywords name="Folders in comment, middle"></Keywords>
            <Keywords name="Folders in comment, close"></Keywords>
            <Keywords name="Keywords1">#yellow&#x000D;&#x000A;@TODO&#x000D;&#x000A;^staging&#x000D;&#x000A;^temp</Keywords>
            <Keywords name="Keywords2">#blue&#x000D;&#x000A;@QUESTION&#x000D;&#x000A;@UPDATE&#x000D;&#x000A;@EXAMPLE&#x000D;&#x000A;^dev</Keywords>
            <Keywords name="Keywords3">#green&#x000D;&#x000A;@COMPLETED&#x000D;&#x000A;@RESOLVED&#x000D;&#x000A;@DONE</Keywords>
            <Keywords name="Keywords4">#orange&#x000D;&#x000A;@IMPORTANT&#x000D;&#x000A;@EXCEPTION&#x000D;&#x000A;^production</Keywords>
            <Keywords name="Keywords5">#red&#x000D;&#x000A;@WARNING&#x000D;&#x000A;@DANGER&#x000D;&#x000A;@ERROR&#x000D;&#x000A;^NoUndo</Keywords>
            <Keywords name="Keywords6">#purple&#x000D;&#x000A;@FUTURE&#x000D;&#x000A;@TOLEARN</Keywords>
            <Keywords name="Keywords7">#pink&#x000D;&#x000A;@NOTE&#x000D;&#x000A;*</Keywords>
            <Keywords name="Keywords8"></Keywords>
            <Keywords name="Delimiters">00` 01 02` 03 04 05 06{ 07 08} 09[ 10 11] 12&lt;code&gt; 13 14&lt;/code&gt; 15$ 16 17$ 18&quot; 19 20&quot; 21~ 22 23~</Keywords>
        </KeywordLists>
        <Styles>
            <WordsStyle name="DEFAULT" fgColor="DEDEDE" bgColor="000000" fontStyle="0" nesting="0" />
            <WordsStyle name="COMMENTS" fgColor="808080" bgColor="000000" fontStyle="2" nesting="261121" />
            <WordsStyle name="LINE COMMENTS" fgColor="808080" bgColor="000000" fontName="" fontStyle="2" nesting="261121" />
            <WordsStyle name="NUMBERS" fgColor="000000" bgColor="FFFFFF" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS1" fgColor="FFFF00" bgColor="000000" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS2" fgColor="379BFF" bgColor="000000" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS3" fgColor="00FF40" bgColor="000000" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS4" fgColor="FF8000" bgColor="000000" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS5" fgColor="FF0000" bgColor="000000" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS6" fgColor="DEDEDE" bgColor="400080" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS7" fgColor="FF00FF" bgColor="000000" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS8" fgColor="000000" bgColor="FFFFFF" fontStyle="0" nesting="0" />
            <WordsStyle name="OPERATORS" fgColor="000000" bgColor="FFFFFF" fontStyle="0" nesting="0" />
            <WordsStyle name="FOLDER IN CODE1" fgColor="000000" bgColor="FFFFFF" fontStyle="0" nesting="0" />
            <WordsStyle name="FOLDER IN CODE2" fgColor="000000" bgColor="FFFFFF" fontStyle="0" nesting="0" />
            <WordsStyle name="FOLDER IN COMMENT" fgColor="000000" bgColor="FFFFFF" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS1" fgColor="2DFFFF" bgColor="000000" fontStyle="0" nesting="776" />
            <WordsStyle name="DELIMITERS2" fgColor="2DFFFF" bgColor="000000" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS3" fgColor="808080" bgColor="000000" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS4" fgColor="808080" bgColor="000000" fontStyle="2" nesting="261121" />
            <WordsStyle name="DELIMITERS5" fgColor="008000" bgColor="000000" fontStyle="0" nesting="261152" />
            <WordsStyle name="DELIMITERS6" fgColor="808080" bgColor="000000" fontStyle="2" nesting="261136" />
            <WordsStyle name="DELIMITERS7" fgColor="C0C0C0" bgColor="000000" fontStyle="2" nesting="0" />
            <WordsStyle name="DELIMITERS8" fgColor="FFFFFF" bgColor="000000" fontStyle="4" nesting="0" />
        </Styles>
    </UserLang>
</NotepadPlus>
